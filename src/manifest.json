{
  "manifest_version": 2,
  "name": "OctoTask",
  "description": "Enhances the experience of managing tasks on GitHub issues",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.github.com/*", "https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}
