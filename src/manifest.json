{
  "manifest_version": 2,
  "name": "Github Project Tools",
  "description": "Adds in some features to GitHub to make it better for project management.",
  "version": "0.0.1",
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["https://www.github.com/*", "https://github.com/*"],
      "js": ["content.js"]
    }
  ],
  "permissions": ["storage"]
}
